import java.util.Scanner;

public class Avg_marks {
public static void main(String args[]) {
Scanner scan=new Scanner(System.in);
int a=scan.nextInt();
	int count=0;
if(a>0) {
	for(int i=1;i<=a;i++) {
		if(a%i==0) {
			count++;
		}
		
	}
	if(count==2) {
		System.out.println("prime");
	}
	else {
		System.out.println("not a prime");
	}
}
}
}
